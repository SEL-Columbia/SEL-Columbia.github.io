{% assign sorted_team = (site.team | sort:"full_name") %}
{% capture authors %}
  {% for member in sorted_team %}
    {% if member.url != "/team/template/" %}
      {"name":"{{ member.full_name | xml_escape }}","value":"{{ member.url | split:"/" | last | xml_escape }}"}|
    {% endif %}
  {% endfor %}
{% endcapture %}
authors([{{ authors | strip | split:"|" | join:","}}])
